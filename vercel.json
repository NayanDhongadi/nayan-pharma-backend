{
    "version": 2,
    "builds": [
      {
        "src": "index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["prisma/schema.prisma", "prisma/migrations/**", "node_modules/.prisma/client/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts"
      }
    ]
  }
  
